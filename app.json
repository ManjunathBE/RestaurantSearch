{
  "expo": {
    "name": "Restaurant Search",
    "slug": "restaurant-search",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "This App suggests the Restaurants based on food or cuisine. Ex: Dosa, Pizza, Pasta. \nNote : Currently location is hard coded as San Jose. "
  }
}
